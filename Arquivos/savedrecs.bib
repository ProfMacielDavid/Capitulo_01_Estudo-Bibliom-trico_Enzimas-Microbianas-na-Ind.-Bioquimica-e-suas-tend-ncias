@incollection{ WOS:000501497400008,
Author = {Laeufer, Albrecht},
Editor = {Wagemann, K and Tippkotter, N},
Title = {Starch Biorefinery Enzymes},
Booktitle = {BIOREFINERIES},
Series = {Advances in Biochemical Engineering-Biotechnology},
Year = {2019},
Volume = {166},
Pages = {137-152},
Abstract = {Nature uses enzymes to build and convert biomass; mankind uses the same
   enzymes and produces them on a large scale to make optimum use of
   biomass in biorefineries. Bacterial alpha-amylases and fungal
   glucoamylases have been the workhorses of starch biorefineries for many
   decades. Pullulanases were introduced in the 1980s. Proteases,
   cellulases, hemicellulases, and phytases have been on the market for a
   few years as process aids, improving yields, performance, and costs.
   Detailed studies of the complex chemical structures of biomass and of
   the physicochemical limitations of industrial biorefineries have led
   enzyme developers to produce novel tailor-made solutions for improving
   yield and profitability in the industry. This chapter reviews the
   development of enzyme applications in the major starch biorefining
   processes.},
