@article{ WOS:000275455000039,
Author = {Um, Byung-Hwan and van Walsum, G. Peter},
Title = {Evaluation of Enzyme Mixtures in Releasing Fermentable Sugars from
   Pre-pulping Extracts of Mixed Northeast Hardwoods},
Journal = {APPLIED BIOCHEMISTRY AND BIOTECHNOLOGY},
