@article{ WOS:000764570100001,
Author = {Pinheiro de Souza, Thaiza Serrano and de Andrade, Cristiano Jose and
   Bello Koblitz, Maria Gabriela and Cavalcante Fai, Ana Elizabeth},
Title = {Microbial Peptidase in Food Processing: Current State of the Art and
   Future Trends},
