@incollection{ WOS:000627111800018,
Author = {Kumar, V. Anoop and Kurup, R. Suresh Chandra and Snishamol, C. and
   Prabhu, G. Nagendra},
Editor = {Parameswaran, B and Varjani, S and Raveendran, S},
Title = {Role of Cellulases in Food, Feed, and Beverage Industries},
Booktitle = {GREEN BIO-PROCESSES: ENZYMES IN INDUSTRIAL FOOD PROCESSING},
