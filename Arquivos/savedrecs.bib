@article{ WOS:001232472200001,
Author = {Oehlenschlaeger, Katharina and Schepp, Emily and Stiefelmaier, Judith
   and Holtmann, Dirk and Ulber, Roland},
Title = {Simultaneous fermentation and enzymatic biocatalysis-a useful process
   option?},
Journal = {BIOTECHNOLOGY FOR BIOFUELS AND BIOPRODUCTS},
Year = {2024},
Volume = {17},
Number = {1},
Month = {MAY 25},
Abstract = {Biotransformation with enzymes and de novo syntheses with whole-cell
   biocatalysts each have specific advantages. These can be combined to
   achieve processes with optimal performance. A recent approach is to
   perform bioconversion processes and enzymatic catalysis simultaneously
   in one-pot. This is a well-established process in the biorefinery, where
   starchy or cellulosic material is degraded enzymatically and
   simultaneously used as substrate for microbial cultivations. This
   procedure leads to a number of advantages like saving in time but also
   in the needed equipment (e.g., reaction vessels). In addition, the
   inhibition or side-reaction of high sugar concentrations can be overcome
   by combining the processes. These benefits of coupling microbial
   conversion and enzymatic biotransformation can also be transferred to
   other processes for example in the sector of biofuel production or in
   the food industry. However, finding a compromise between the different
   requirements of the two processes is challenging in some cases. This
   article summarises the latest developments and process variations.},
