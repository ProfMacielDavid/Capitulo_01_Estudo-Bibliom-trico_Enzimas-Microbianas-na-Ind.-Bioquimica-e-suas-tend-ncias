@article{ WOS:001066753600001,
Author = {Bishnoi, Shreya and Sharma, Shweta and Agrawal, Himani},
Title = {Exploration of the Potential Application of Banana Peel for Its
   Effective Valorization: A Review},
Journal = {INDIAN JOURNAL OF MICROBIOLOGY},
